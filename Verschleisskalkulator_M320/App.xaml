<Application x:Class="Verschleisskalkulator_M320.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Verschleisskalkulator_M320"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="MSCFont">
            <Setter Property="TextElement.FontFamily" Value="/Fonts/#Alfa Slab One"/>
            <Setter Property="TextElement.FontStyle" Value="Italic"/>
            <Setter Property="Block.Foreground" Value="#FFCACC1D"/>
        </Style>
    </Application.Resources>
</Application>
